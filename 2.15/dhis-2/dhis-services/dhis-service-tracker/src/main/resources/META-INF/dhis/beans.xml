			<aop:before
				pointcut="execution( * org.hisp.dhis.trackedentity.TrackedEntityService.delete*(..) )"
				method="intercept" />
			<aop:before
				pointcut="execution( * org.hisp.dhis.trackedentity.TrackedEntityInstanceService.delete*(..) )"
